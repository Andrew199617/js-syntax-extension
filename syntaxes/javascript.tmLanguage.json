{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "LGDJavascript",
	"scopeName": "source.lgd.js",
	"injectionSelector": "L:source.js",
	"patterns": [
		{
			"include": "#reactBuiltin"
		}
	],
	"repository": {
		"reactBuiltin": {
			"match": "(?<!\\s)([A-Z]+\\w+)(\\.)(propTypes|defaultProps|getInitialProps)",
			"captures": {
				"1": {
					"name": "support.class.proptypes"
				},
				"2": {
					"name": "punctuation.accessor.js"
				},
				"3": {
					"patterns": [
						{
							"include": "#builtins"
						}
					]
				}
			}
		},
		"builtins": {
			"match": "(propTypes|defaultProps)|(getInitialProps|getStaticProps|getStaticPaths|getServerSideProps)",
			"captures": {
				"1": {
					"name": "variable.react.builtins"
				},
				"2": {
					"name": "variable.next.builtins"
				}
			}
		}
	}
}